{
    "manifest_version": 3,
    "name": "SCRAPE_CHROME",
    "version": "1.0",
    "description": "written by O-79 // Adi",
    "permissions": [
        "activeTab",
        "scripting",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "resources/ICO-SCRAPE_CHROME_16.png",
            "48": "resources/ICO-SCRAPE_CHROME_48.png",
            "128": "resources/ICO-SCRAPE_CHROME_128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
      "16": "resources/ICO-SCRAPE_CHROME_16.png",
      "48": "resources/ICO-SCRAPE_CHROME_48.png",
      "128": "resources/ICO-SCRAPE_CHROME_128.png"
    }
}